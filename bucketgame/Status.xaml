        <Label Content="Time: 00:00.00" x:Name="LabelTime" HorizontalAlignment="Left" FontSize="30" Height="53" Margin="10,10,0,0" VerticalAlignment="Top" Width="264"/>
        <Label Content="Score: 0" x:Name="LabelScore" HorizontalAlignment="Left" FontSize="30" Height="53" Margin="10,63,0,0" VerticalAlignment="Top" Width="264"/>
        <Label Content="" x:Name="LabelNothing" HorizontalAlignment="Left" FontSize="30" Height="53" Margin="10,239,0,0" VerticalAlignment="Top" Width="264"/>
        <Label Content="Use " x:Name="LabelJoint" HorizontalAlignment="Left" FontSize="30" Height="53" Margin="10,121,0,0" VerticalAlignment="Top" Width="264"/>

    </Grid>
</Window>
